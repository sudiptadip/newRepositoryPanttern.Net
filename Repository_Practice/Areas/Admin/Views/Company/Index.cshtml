@model List<Company>


<div class="container">
    <div class="row justify-content-center">
        <h1 class="text-center" >Company List</h1>
        <div class="col-6">
            <a class="btn btn-primary mt-4 mb-3" asp-controller="Company" asp-action="Create">Create Product</a>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">Name</th>
                        <th scope="col">Address</th>
                        <th scope="col">City</th>
                        <th scope="col">State</th>
                    </tr>
                </thead>
                <tbody>

                    @foreach (var obj in Model)
                    {
                        <tr>
                            <th scope="row">@obj.Id</th>
                            <td>@obj.Name</td>
                            <td>@obj.StreetAddress</td>
                            <td>@obj.City</td>
                            <td>@obj.State</td>>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>